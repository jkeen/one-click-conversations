	<string>Access Gmail conversations in one painless click.</string>
	<key>ExtensionInfoDictionaryVersion</key>
	<string>1.0</string>
	<key>Permissions</key>
	<dict>
		<key>Website Access</key>
		<dict>
			<key>Allowed Domains</key>
			<array>
				<string>mail.google.com</string>
			</array>
			<key>Include Secure Pages</key>
			<true/>
			<key>Level</key>
			<string>Some</string>
		</dict>
	</dict>
	<key>Update Manifest URL</key>
	<string>http://github.com/jkeen/one-click-conversations/raw/master/safari-extension-update-manifest.plist</string>
	<key>Website</key>
	<string>http://keen.me</string>
